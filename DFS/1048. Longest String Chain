class Solution:
    def longestStrChain(self, words: List[str]) -> int: 
        #DFS memo 
        wordSet = set(words) 
        memo = defaultdict(int) 
        def dfs(word): 
            maxLength = 1 
            if word in memo: 
                return memo[word]
            for i in range(len(word)): 
                newWord = word[ : i] + word[i + 1 : ]
                if newWord in wordSet: 
                    maxLength = max(1 + dfs(newWord), maxLength)
            memo[word] = maxLength 
            return maxLength 
        res = 0
        for word in words: 
            res = max(res, dfs(word))
        return res 
