def myPow(self, x: float, n: int) -> float:
        #O(logN): (x^n)^2 = x^(2n) time and O(logN) space 
        if n == 0:
            return 1 
        if n < 0:
            return self.myPow(1/x, -n) 
        if n % 2 != 0: 
            return x * self.myPow(x, n - 1)
        #(x^2)^n/2 = x^(2*n/2) = x^n
        return self.myPow(x * x, n/2)
